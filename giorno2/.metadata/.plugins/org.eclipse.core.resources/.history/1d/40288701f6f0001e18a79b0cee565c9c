package whileEs;

import java.util.Scanner;

public class WhileEx {

	public static void main(String[] args) {
		Scanner x = new Scanner(System.in);
		int tot = 0;
		
		while(true) {
			
			int r = x.nextInt();
			tot += r;
			if ( r == 0) {
				
				System.out.println(r);
				break;
			}
				
		}
		x.close();
	}

}
