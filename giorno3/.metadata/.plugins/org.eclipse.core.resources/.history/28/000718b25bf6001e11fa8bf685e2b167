package liste;

import java.util.ArrayList;
import java.util.Collection;

public class Liste {

	public static void main(String[] args) {
		
	List<String> l = new ArrayList<String>();
		
		l.add("car");
		l.add("bike");
		l.addAll(l);
		l.remove("car");
		System.out.println(l);
		
		System.out.println(l.isEmpty());
		System.out.println(l.);
		l.clear();
		System.out.println(l);

	}

}

